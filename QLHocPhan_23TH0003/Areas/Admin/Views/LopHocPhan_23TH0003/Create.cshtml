@model QLHocPhan_23TH0003.Models.LopHocPhan
@{
    ViewBag.Title = "Thêm lớp học phần";
}
<h4>@ViewBag.Title</h4>
@Html.Partial("_AlertMessages")
        <form action="@Url.Action("Create", "LopHocPhan_23TH0003")" method="post">
            @Html.AntiForgeryToken()
            @{
                ViewData["HocPhan"] = ViewBag.IdHocPhan;
            }
            @Html.Partial("_Fields", Model, ViewData)
            <div class="form-group">
                <input type="submit" value="Lưu" class="btn btn-primary" />
            </div>
        </form>
    


